<div class="container">
    <div class="py-5 text-center">
        <img class="d-block mx-auto mb-4" src="https://finandy.com/icons/logo2@2x.png" alt="" width="72" height="72">
        <h2 id="pagetitle">Finandy Telegram Commands</h2>
    </div>

    <div class="row">
        <div class="col order-md-2 mb-4">
            <h4 class="d-flex justify-content-between align-items-center mb-3">
                <span class="text-muted">Command Flow</span>
                <button class="btn btn-primary" onclick="clearCommandFlow()" ><span class="badge badge-secondary badge-pill command_count">0 </span> <i class="fa-solid fa-trash"  data-toggle="tooltip" data-placement="top" title="Clear Command Flow"></i></button>
                <button class="btn btn-primary" onclick="uploadCommandFlow()" ><i class="fa-solid fa-upload"  data-toggle="tooltip" data-placement="top" title="Upload an Command Flow"></i></button>
                <button class="btn btn-primary" onclick="downloadCommandFlow()" ><i class="fa-solid fa-download"  data-toggle="tooltip" data-placement="top" title="Download the Command Flow"></i></button>
            </h4>
            <ul class="list-group mb-3 command_elem_list">
            </ul>
        </div>
    </div>
</div>